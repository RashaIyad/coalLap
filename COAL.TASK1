.MODEL SMALL
.STACK 100H
.DATA

.CODE
MAIN PROC
    MOV AX, @DATA
    MOV DS, AX

    ; Print H
    MOV DL, 'H'
    MOV AH, 02H
    INT 21H

    ; Print E
    MOV DL, 'E'
    MOV AH, 02H
    INT 21H

    ; Print L
    MOV DL, 'L'
    MOV AH, 02H
    INT 21H

    ; Print L
    MOV DL, 'L'
    MOV AH, 02H
    INT 21H

    ; Print O
    MOV DL, 'O'
    MOV AH, 02H
    INT 21H

    MOV AH, 4CH
    INT 21H

MAIN ENDP
END MAIN
